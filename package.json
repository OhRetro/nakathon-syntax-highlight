{
	"name": "nakathon-syntax-highlight",
	"displayName": "Nakathon Syntax Highlight",
	"description": "An Syntax Highlight for my programming language, Nakathon",
	"publisher": "ohretro-naka",
	"version": "0.0.6",
	"icon": "./assets/logo_icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/OhRetro/nakathon-syntax-highlight.git"
	},
	"engines": {
		"vscode": "^1.86.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "nakathon",
				"aliases": [
					"Nakathon",
					"nakathon"
				],
				"extensions": [
					".nkt"
				],
				"icon": {
					"light": "./assets/logo_icon.png",
					"dark": "./assets/logo_icon.png"
				},
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "nakathon",
				"scopeName": "source.nkt",
				"path": "./syntaxes/nakathon.tmLanguage.json"
			}
		]
	}
}